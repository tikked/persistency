      "text"
    ],
    "temp-dir": "./.nyc_output/",
    "all": true
  },
  "scripts": {
    "build": "tsc",
    "clean": "rimraf dist .nyc_output coverage",
    "count": "cloc --by-file-by-lang src",
    "coverage": "nyc npm test",
    "lint-fix": "eslint --fix ./src/**",
